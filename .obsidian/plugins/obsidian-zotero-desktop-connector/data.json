{
  "database": "Zotero",
  "noteImportFolder": "Inbox/",
  "pdfExportImageDPI": 120,
  "pdfExportImageFormat": "jpg",
  "pdfExportImageQuality": 90,
  "citeFormats": [
    {
      "name": "Cite",
      "format": "formatted-citation"
    },
    {
      "name": "Bibliography",
      "format": "formatted-bibliography",
      "cslStyle": "harvard-melbourne-polytechnic"
    }
  ],
  "exportFormats": [
    {
      "name": "Melbourne Polytechnic",
      "outputPathTemplate": "Z_IN/{{citekey}}/{{citekey}}.md",
      "imageOutputPathTemplate": "Z_IN/{{citekey}}/assets",
      "imageBaseNameTemplate": "img",
      "cslStyle": "harvard-melbourne-polytechnic",
      "templatePath": "Z_Templates/assets/Paper.md"
    }
  ],
  "citeSuggestTemplate": "[[{{citekey}}]]",
  "openNoteAfterImport": true,
  "whichNotesToOpenAfterImport": "first-imported-note"
}